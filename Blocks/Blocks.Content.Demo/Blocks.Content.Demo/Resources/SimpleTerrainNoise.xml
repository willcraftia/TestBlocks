			<Properties>
				<Property Name="Name" Value="Cave Perturb" />
				<Property Name="Source" Value="CaveShapeAttenuate" />
				<Property Name="DisplaceX" Value="CavePerturbScaleBias" />
				<Property Name="DisplaceY" Value="ConstZero" />
				<Property Name="DisplaceZ" Value="ConstZero" />
			</Properties>
		</Component>
		<Component Name="CaveDensity" Type="Select">
			<Properties>
				<Property Name="Name" Value="Cave Density" />
				<Property Name="Controller" Value="CavePerturb" />
				<Property Name="LowerSource" Value="ConstOne" />
				<Property Name="UpperSource" Value="ConstZero" />
